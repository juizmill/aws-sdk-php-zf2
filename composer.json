{
    "name": "juizmill/aws-sdk-php-zf2",
    "description": "Zend Framework 2 Module that allows easy integration the AWS SDK for PHP",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "aws",
        "zf2",
        "amazon",
        "zend",
        "s3",
        "dynamodb",
        "ec2",
        "cloudfront",
        "sqs"
    ],
    "require": {
        "php": ">=8.0",
        "aws/aws-sdk-php": "3.*",
        "laminas/laminas-filter": "2.*",
        "laminas/laminas-servicemanager": "3.*",
        "laminas/laminas-session": "2.*",
        "laminas/laminas-view": "2.*",
        "laminas/laminas-dependency-plugin": "2.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "laminas/laminas-modulemanager": "2.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "AwsModule\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AwsModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "module": "AwsModule"
        }
    }
}
